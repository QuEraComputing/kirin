impl ::kirin::pretty::PrettyPrint<Load> for Load {
    fn pretty_print<'a>(
        &self,
        doc: &'a ::kirin::pretty::Document<'a, Load>,
    ) -> ::kirin::pretty::ArenaDoc<'a> {
        use kirin::pretty::DocAllocator;
        let Load {
            addr: f_addr,
            res: f_res,
        } = self;
        ::kirin::pretty::PrettyPrintName::pretty_print_name(f_res, doc)
            + doc.text(" = load [")
            + ::kirin::pretty::PrettyPrint::pretty_print(f_addr, doc)
            + doc.text("] -> ")
            + ::kirin::pretty::PrettyPrintType::pretty_print_type(f_res, doc)
    }
}
