impl ::kirin::pretty::PrettyPrint<SimpleLang> for SimpleLang {
    fn pretty_print<'a>(
        &self,
        doc: &'a ::kirin::pretty::Document<'a, SimpleLang>,
    ) -> ::kirin::pretty::ArenaDoc<'a> {
        use kirin::pretty::DocAllocator;
        match self {
            SimpleLang::Add {
                lhs: f_lhs,
                rhs: f_rhs,
                res: f_res,
            } => {
                ::kirin::pretty::PrettyPrintName::pretty_print_name(f_res, doc)
                    + doc.text(" = add ")
                    + ::kirin::pretty::PrettyPrint::pretty_print(f_lhs, doc)
                    + doc.text(", ")
                    + ::kirin::pretty::PrettyPrint::pretty_print(f_rhs, doc)
                    + doc.text(" -> ")
                    + ::kirin::pretty::PrettyPrintType::pretty_print_type(f_res, doc)
            }
            SimpleLang::Return { arg: f_arg } => {
                doc.text("return ") + ::kirin::pretty::PrettyPrint::pretty_print(f_arg, doc)
            }
        }
    }
}
