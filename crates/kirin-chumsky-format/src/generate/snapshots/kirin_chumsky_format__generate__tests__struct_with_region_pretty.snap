impl ::kirin::pretty::PrettyPrint for Function {
    fn pretty_print<'a, __L: ::kirin::ir::Dialect + ::kirin::pretty::PrettyPrint>(
        &self,
        doc: &'a ::kirin::pretty::Document<'a, __L>,
    ) -> ::kirin::pretty::ArenaDoc<'a>
    where
        __L::TypeLattice: ::core::fmt::Display,
    {
        use kirin::pretty::DocAllocator;
        let Function {
            body: f_body,
            res: f_res,
        } = self;
        ::kirin::pretty::PrettyPrintName::pretty_print_name(f_res, doc)
            + doc.text(" = function ")
            + doc.print_region(f_body)
            + doc.text(" -> ")
            + ::kirin::pretty::PrettyPrintType::pretty_print_type(f_res, doc)
    }
}
