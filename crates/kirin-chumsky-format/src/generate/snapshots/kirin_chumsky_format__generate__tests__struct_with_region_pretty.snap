impl ::kirin_prettyless::PrettyPrint<Function> for Function {
    fn pretty_print<'a>(
        &self,
        doc: &'a ::kirin_prettyless::Document<'a, Function>,
    ) -> ::kirin_prettyless::ArenaDoc<'a> {
        use kirin_prettyless::DocAllocator;
        let Function {
            res: f_res,
            body: f_body,
        } = self;
        ::kirin_prettyless::PrettyPrintName::pretty_print_name(f_res, doc)
            + doc.text(" = function ")
            + ::kirin_prettyless::PrettyPrint::pretty_print(f_body, doc)
            + doc.text(" -> ")
            + ::kirin_prettyless::PrettyPrintType::pretty_print_type(f_res, doc)
    }
}
