impl ::kirin::pretty::PrettyPrint<Function> for Function {
    fn pretty_print<'a>(
        &self,
        doc: &'a ::kirin::pretty::Document<'a, Function>,
    ) -> ::kirin::pretty::ArenaDoc<'a> {
        use kirin::pretty::DocAllocator;
        let Function {
            body: f_body,
            res: f_res,
        } = self;
        ::kirin::pretty::PrettyPrintName::pretty_print_name(f_res, doc)
            + doc.text(" = function ")
            + ::kirin::pretty::PrettyPrint::pretty_print(f_body, doc)
            + doc.text(" -> ")
            + ::kirin::pretty::PrettyPrintType::pretty_print_type(f_res, doc)
    }
}
