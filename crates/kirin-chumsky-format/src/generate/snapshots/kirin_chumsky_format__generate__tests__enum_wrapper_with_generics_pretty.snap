{
    fn pretty_print<'a>(
        &self,
        doc: &'a ::kirin::pretty::Document<'a, WrapperWithGenerics<T>>,
    ) -> ::kirin::pretty::ArenaDoc<'a> {
        use kirin::pretty::DocAllocator;
        match self {
            WrapperWithGenerics::Inner(inner) => {
                let doc_inner: &'a ::kirin::pretty::Document<'a, GenericInner<T>> =
                    unsafe { ::core::mem::transmute(doc) };
