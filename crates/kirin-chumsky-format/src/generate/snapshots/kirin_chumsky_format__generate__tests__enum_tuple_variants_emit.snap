impl<'tokens, 'src: 'tokens, TypeOutput, LanguageOutput, Language>
    ::kirin::parsers::EmitIR<Language> for TupleLangAST<'tokens, 'src, TypeOutput, LanguageOutput>
where
    Language: ::kirin::ir::Dialect + From<TupleLang>,
    TypeOutput: Clone + PartialEq,
    LanguageOutput: Clone + PartialEq + 'tokens,
    SimpleType: ::kirin::parsers::HasParser<'tokens, 'src> + 'tokens,
    <SimpleType as ::kirin::parsers::HasParser<'tokens, 'src>>::Output:
        ::kirin::parsers::EmitIR<Language, Output = <Language as ::kirin::ir::Dialect>::Type>,
{
    type Output = ::kirin::ir::Statement;
    fn emit(&self, ctx: &mut ::kirin::parsers::EmitContext<'_, Language>) -> Self::Output {
        match self {
            TupleLangAST::Return(f0) => {
                let f0_ir = ::kirin::parsers::EmitIR::emit(f0, ctx);
                let dialect_variant: TupleLang = TupleLang::Return(f0_ir.into());
                ctx.stage.statement().definition(dialect_variant).new()
            }
            TupleLangAST::Neg(f0, f1) => {
                let f0_ir = ::kirin::parsers::EmitIR::emit(f0, ctx);
                let f1_ir = ::kirin::parsers::EmitIR::emit(f1, ctx);
                let dialect_variant: TupleLang = TupleLang::Neg(f0_ir.into(), f1_ir.into());
                ctx.stage.statement().definition(dialect_variant).new()
            }
            TupleLangAST::__Marker(_, unreachable) => match *unreachable {},
        }
    }
}
