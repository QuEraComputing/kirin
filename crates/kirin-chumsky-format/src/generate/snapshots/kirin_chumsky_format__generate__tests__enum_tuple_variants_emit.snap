---
source: crates/kirin-chumsky-format/src/generate/tests.rs
expression: emit
---
impl<'tokens, 'src: 'tokens, Language: ::kirin_ir::Dialect> ::kirin_chumsky::EmitIR<Language>
    for TupleLangAST<'tokens, 'src, Language>
where
    Language: ::kirin_ir::Dialect + From<TupleLang>,
    <SimpleType as ::kirin_chumsky::HasParser<'tokens, 'src>>::Output:
        ::kirin_chumsky::EmitIR<Language, Output = <Language as ::kirin_ir::Dialect>::TypeLattice>,
{
    type Output = ::kirin_ir::Statement;
    fn emit(&self, ctx: &mut ::kirin_chumsky::EmitContext<'_, Language>) -> Self::Output {
        match self {
            TupleLangAST::Return(f0) => {
                let f0_ir: ::kirin_ir::SSAValue = ::kirin_chumsky::EmitIR::emit(f0, ctx);
                let dialect_variant: TupleLang = TupleLang::Return(f0_ir.into());
                ctx.context.statement().definition(dialect_variant).new()
            }
            TupleLangAST::Neg(f0, f1) => {
                let f0_ir: ::kirin_ir::SSAValue = ::kirin_chumsky::EmitIR::emit(f0, ctx);
                let f1_ir: ::kirin_ir::ResultValue = ::kirin_chumsky::EmitIR::emit(f1, ctx);
                let dialect_variant: TupleLang = TupleLang::Neg(f0_ir.into(), f1_ir.into());
                ctx.context.statement().definition(dialect_variant).new()
            }
            TupleLangAST::__Marker(_, unreachable) => match *unreachable {},
        }
    }
}
