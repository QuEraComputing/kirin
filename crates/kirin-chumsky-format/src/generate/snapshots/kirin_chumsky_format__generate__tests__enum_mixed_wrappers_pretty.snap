impl ::kirin::pretty::PrettyPrint<MixedLang> for MixedLang {
    fn pretty_print<'a>(
        &self,
        doc: &'a ::kirin::pretty::Document<'a, MixedLang>,
    ) -> ::kirin::pretty::ArenaDoc<'a> {
        use kirin::pretty::DocAllocator;
        match self {
            MixedLang::Arith(inner) => {
                let doc_inner: &'a ::kirin::pretty::Document<'a, ArithDialect> =
                    unsafe { ::core::mem::transmute(doc) };
                ::kirin::pretty::PrettyPrint::pretty_print(inner, doc_inner)
            }
            MixedLang::Custom {
                arg: f_arg,
                res: f_res,
            } => {
                ::kirin::pretty::PrettyPrintName::pretty_print_name(f_res, doc)
                    + doc.text(" = custom ")
                    + ::kirin::pretty::PrettyPrint::pretty_print(f_arg, doc)
                    + doc.text(" -> ")
                    + ::kirin::pretty::PrettyPrintType::pretty_print_type(f_res, doc)
            }
            MixedLang::Control(inner) => {
                let doc_inner: &'a ::kirin::pretty::Document<'a, ControlDialect> =
                    unsafe { ::core::mem::transmute(doc) };
                ::kirin::pretty::PrettyPrint::pretty_print(inner, doc_inner)
            }
        }
    }
}
