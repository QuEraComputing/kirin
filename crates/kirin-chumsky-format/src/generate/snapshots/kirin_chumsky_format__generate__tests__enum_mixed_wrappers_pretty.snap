impl ::kirin::pretty::PrettyPrint for MixedLang {
    fn pretty_print<'a, __L: ::kirin::ir::Dialect + ::kirin::pretty::PrettyPrint>(
        &self,
        doc: &'a ::kirin::pretty::Document<'a, __L>,
    ) -> ::kirin::pretty::ArenaDoc<'a>
    where
        __L::Type: ::core::fmt::Display,
    {
        use kirin::pretty::DocAllocator;
        match self {
            MixedLang::Arith(inner) => ::kirin::pretty::PrettyPrint::pretty_print(inner, doc),
            MixedLang::Custom {
                arg: f_arg,
                res: f_res,
            } => {
                ::kirin::pretty::PrettyPrintName::pretty_print_name(f_res, doc)
                    + doc.text(" = custom ")
                    + ::kirin::pretty::PrettyPrint::pretty_print(f_arg, doc)
                    + doc.text(" -> ")
                    + ::kirin::pretty::PrettyPrintType::pretty_print_type(f_res, doc)
            }
            MixedLang::Control(inner) => ::kirin::pretty::PrettyPrint::pretty_print(inner, doc),
        }
    }
}
