impl ::kirin::pretty::PrettyPrint<TupleLang> for TupleLang {
    fn pretty_print<'a>(
        &self,
        doc: &'a ::kirin::pretty::Document<'a, TupleLang>,
    ) -> ::kirin::pretty::ArenaDoc<'a> {
        use kirin::pretty::DocAllocator;
        match self {
            TupleLang::Return(f0) => {
                doc.text("return ") + ::kirin::pretty::PrettyPrint::pretty_print(f0, doc)
            }
            TupleLang::Neg(f0, f1) => {
                ::kirin::pretty::PrettyPrintName::pretty_print_name(f1, doc)
                    + doc.text(" = neg ")
                    + ::kirin::pretty::PrettyPrint::pretty_print(f0, doc)
                    + doc.text(" -> ")
                    + ::kirin::pretty::PrettyPrintType::pretty_print_type(f1, doc)
            }
        }
    }
}
