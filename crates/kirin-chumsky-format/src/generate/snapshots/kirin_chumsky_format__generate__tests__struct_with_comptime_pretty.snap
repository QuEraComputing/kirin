impl ::kirin::pretty::PrettyPrint<Constant> for Constant {
    fn pretty_print<'a>(
        &self,
        doc: &'a ::kirin::pretty::Document<'a, Constant>,
    ) -> ::kirin::pretty::ArenaDoc<'a> {
        use kirin::pretty::DocAllocator;
        let Constant {
            value: f_value,
            res: f_res,
        } = self;
        ::kirin::pretty::PrettyPrintName::pretty_print_name(f_res, doc)
            + doc.text(" = constant ")
            + ::kirin::pretty::PrettyPrint::pretty_print(f_value, doc)
            + doc.text(" -> ")
            + ::kirin::pretty::PrettyPrintType::pretty_print_type(f_res, doc)
    }
}
