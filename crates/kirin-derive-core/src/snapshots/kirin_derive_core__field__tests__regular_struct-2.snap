impl<'a, T> kirin_ir::HasArguments<'a> for TestStruct<T> {
    type Iter = std::iter::Chain<
        std::iter::Once<&'a kirin_ir::SSAValue>,
        std::iter::Once<&'a kirin_ir::SSAValue>,
    >;
    fn arguments(&'a self) -> Self::Iter {
        let Self { a, b, .. } = self;
        std::iter::once(a).chain(std::iter::once(b))
    }
}
